# Example environment variables for local development
# Copy this file to .env and fill values

# MySQL connection (used by server and drizzle)
# Format: mysql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL=mysql://root:password@localhost:3306/decatdb

# OAuth server (for development you can use http://localhost:3000)
OAUTH_SERVER_URL=http://localhost:3000

# Other optional Vite envs (examples)
VITE_APP_TITLE=DecatAPP
VITE_APP_LOGO=
VITE_ANALYTICS_ENDPOINT=
VITE_ANALYTICS_WEBSITE_ID=
